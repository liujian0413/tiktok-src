<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/a_0"/>
        </shape>
    </item>
    <item android:gravity="top" android:drawable="@color/a_0" android:bottom="95dp"/>
    <item android:bottom="25dp">
        <bitmap android:gravity="bottom|center_vertical|center_horizontal|center" android:src="@drawable/splash_activity_banner"/>
    </item>
    <item android:top="30dp" android:right="30dp">
        <bitmap android:gravity="top|right|center_vertical|center_horizontal|center" android:src="@drawable/splash"/>
    </item>
</layer-list>
