<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="6dp"/>
            <gradient android:startColor="@color/a5o" android:endColor="@color/a5o" android:angle="0" android:centerX="0.5" android:centerColor="@color/a5o"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="6dp"/>
                <gradient android:startColor="#4cface15" android:endColor="@color/a71" android:angle="0" android:centerX="0.5" android:centerColor="@color/a71"/>
            </shape>
        </clip>
    </item>
</layer-list>
